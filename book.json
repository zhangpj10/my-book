{
  "title": "学习笔记",
  "author": "zhangpj",
  "description": "前端学习笔记文档",
  "language": "zh-hans",
  "gitbook": "3.2.3",
  "structure": {
    "readme": "README.md"
  },
  "link": {
    "sidebar": {
      "我的博客": "https://www.cnblogs.com/pjblog-code/"
    }
  },
  "plugins": [
    "-sharing",
    "splitter",
    "expandable-chapters-small",
    "anchors",
    "disqus",
    "advanced-emoji",
    "tbfed-pagefooter",
    "copy-code-button",

    "github",
    "github-buttons",
    "donate",
    "sharing-plus",
    "anchor-navigation-ex"
  ],
  "pluginsConfig": {
    "disqus": {
      "shortName": "gitbookuse"
    },
    "tbfed-pagefooter": {
      "copyright":"Copyright &copy zhangpj.com 2018",
      "modify_label": "该文件修订时间：",
      "modify_format": "YYYY-MM-DD HH:mm:ss"
    },
    "github": {
      "url": "https://github.com/zhangpj10"
    },
    "github-buttons": {
      "buttons": [{
        "user": "zhangpj",
        "repo": "zhangpj10/my-book",
        "type": "star",
        "size": "small",
        "count": true
      }]
    },
    "donate": {
      "alipay": "./source/images/alipay.jpg",
      "title": "",
      "button": "赏",
      "alipayText": "支付宝打赏"
    },
    "sharing": {
      "google": false,
      "qq": false,
      "qzone": false,
      "twitter": false,
      "viber": false,
      "vk": false,
      "weibo": false,
      "whatsapp": false,
      "all": [
        "google", "facebook", "weibo", "twitter",
        "qq", "qzone", "linkedin", "pocket"
      ]
    },
    "anchor-navigation-ex": {
      "showLevel": false
    }
  }
}